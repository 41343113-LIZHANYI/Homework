#include <iostream>
using namespace std;
unsigned long long Ack(unsigned long long m,unsigned long long n){
    if(m==0)
        return n+1; //遞迴結束條件
    if(n==0)
        return Ack(m-1,1);
    return Ack(m-1,Ack(m,n-1));
}
int main(){
    int m,n;
    while(cin>>m>>n)
        cout<<Ack(m,n)<<'\n';
}
